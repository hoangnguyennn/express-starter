{
  "name": "express-starter",
  "version": "1.0.0",
  "author": "hoangnguyen <nvanhoang188@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "tsnd -r tsconfig-paths/register src/server.ts",
    "build": "rm -rf dist && tsc && tsc-alias",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "prettier": "npx prettier src --check",
    "prettier:fix": "yarn prettier --write",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "yarn prettier:fix && yarn lint --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "bcrypt": "5.0.1",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "jsonwebtoken": "8.5.1",
    "mongoose": "6.2.7",
    "multer": "1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.13",
    "@types/jsonwebtoken": "8.5.8",
    "@types/multer": "1.4.7",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "cross-env": "7.0.3",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.0",
    "lint-staged": "13.1.0",
    "prettier": "2.6.0",
    "ts-node-dev": "1.1.8",
    "tsc-alias": "1.6.4",
    "tsconfig-paths": "3.14.0",
    "typescript": "4.6.2"
  }
}
